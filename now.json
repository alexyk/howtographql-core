{
  "version": 2,
  "name": "api",
  "scope": "howtographql",
  "alias": "api.howtogaphql.now.sh",
  "env": {
    "NODE_ENV": "dev"
  },
  "builds": [
    {
      "src": "packages/server/dist/src/index.js",
      "use": "@now/node-server@canary"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/server/dist/src/index.js"
    }
  ]
}